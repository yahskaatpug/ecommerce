    <!-- Wrapper for slides -->
    <div class="carousel-inner" role="listbox">

      <div class="item active">
        <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcRmast5_t9ec6F9mf5n7Hye2QgiokQDyxE3mCzWh57fDP7ufhTy" width="460" height="345">
